{
  "extends": "@gymter/typescript-config/nextjs.json",
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
