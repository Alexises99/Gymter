{
  "extends": "@gymter/typescript-config/nextjs.json",
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "paths": {
      "@/*": ["./*"],
      "@/app/*": ["./app/*"],
      "@/components/*": ["app/(protected)/home/components/*"],
      "@/utils/*": ["./utils/*"],
      "@/types/*": ["./types/*"],
      "@/stories/*": ["./stories/*"],
      "@/i18n/*": ["./i18n/*"],
      "@/messages/*": ["./messages/*"],
      "@/public/*": ["./public/*"]
    },
    "baseUrl": ".",
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
